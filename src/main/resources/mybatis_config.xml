<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="spring.biz.user.vo.UserVO" alias="uservo" />
		<typeAlias type="spring.biz.product.vo.ProductVO" alias="prodvo" />
		<typeAlias type="spring.biz.category.vo.CategoryVO" alias="catevo" />		
		<typeAlias type="spring.biz.orderinfo.vo.OrderinfoVO" alias="orderinfovo" />		
	</typeAliases>

	<mappers>
		<mapper resource="mapper/user_mapper.xml" />
		<mapper resource="mapper/product_mapper.xml" />
		<mapper resource="mapper/category_mapper.xml" />
		<mapper resource="mapper/orderinfo_mapper.xml" />

	</mappers>
</configuration>